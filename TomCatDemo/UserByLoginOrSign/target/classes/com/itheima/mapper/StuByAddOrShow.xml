<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.itheima.mapper.StuByAddOrShow">


    <insert id="addStu">
          INSERT into stu (stuName,stuAge,stuScore) VALUES (#{stuName},#{stuAge},#{stuScore})
    </insert>
</mapper>